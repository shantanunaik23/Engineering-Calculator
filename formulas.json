[
  {
    "id": "rect_area",
    "name": "Rectangle Area",
    "description": "Area of a rectangle",
    "expression": "width * height",
    "inputs": [ { "name": "width", "type": "number" }, { "name": "height", "type": "number" } ]
  },
  {
    "id": "circle_area",
    "name": "Circle Area",
    "description": "Area of a circle",
    "expression": "Math.PI * radius * radius",
    "inputs": [ { "name": "radius", "type": "number" } ]
  },
  {
    "id": "triangle_area",
    "name": "Triangle Area",
    "description": "Area of a triangle",
    "expression": "0.5 * base * height",
    "inputs": [ { "name": "base", "type": "number" }, { "name": "height", "type": "number" } ]
  },
  {
    "id": "rect_perimeter",
    "name": "Rectangle Perimeter",
    "description": "Perimeter of a rectangle",
    "expression": "2 * (width + height)",
    "inputs": [ { "name": "width", "type": "number" }, { "name": "height", "type": "number" } ]
  }
]
